{
  "name": "Dutee",
  "version": "1.0.0",
  "description": "Fair duty schedule",
  "main": "bundle.js",
  "dependencies": {
    "firebase": "^5.2.0",
    "firebaseui": "^3.1.1",
    "maquette": "3.3.0"
  },
  "devDependencies": {
    "@types/chai": "4.1.4",
    "@types/mocha": "5.2.4",
    "@types/node": "10.5.2",
    "@types/redux": "3.6.0",
    "@types/sinon": "5.0.1",
    "@types/sinon-chai": "3.2.0",
    "chai": "4.1.2",
    "maquette-query": "0.11.7",
    "mocha": "5.2.0",
    "nyc": "^13.0.0",
    "redux": "4.0.0",
    "sinon": "6.1.3",
    "sinon-chai": "3.2.0",
    "ts-loader": "4.4.2",
    "ts-node": "7.0.0",
    "tslint": "5.10.0",
    "typescript": "2.9",
    "webpack": "4.16.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.4"
  },
  "scripts": {
    "test": "mocha test/**/*.ts --require ts-node/register",
    "coverage": "nyc --check-coverage --statements 100 --lines 100 --functions 100 --branches 100 mocha test/**/*.ts --require ts-node/register",
    "dev": "webpack-dev-server",
    "lint": "tslint --type-check --project ./tsconfig.json ./src/**/*.ts",
    "precommit": "npm run lint && npm run test",
    "prepush": "npm run lint && npm run coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peterwestendorp/dutee.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/peterwestendorp/dutee/issues"
  },
  "homepage": "https://github.com/peterwestendorp/dutee#readme",
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html",
      "text"
    ]
  }
}
